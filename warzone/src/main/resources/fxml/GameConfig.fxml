<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="622.0" prefWidth="662.0" style="-fx-background-color: #EDF1FF;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soen6441.warzone.controller.GameConfigController">
   <children>
      <Button fx:id="d_StartGame" layoutX="56.0" layoutY="29.0" mnemonicParsing="false" onAction="#toStartGame" prefHeight="30.0" prefWidth="127.0" text="Start Game">
         <font>
            <Font name="System Italic" size="14.0" />
         </font></Button>
      <Button fx:id="d_Back" layoutX="493.0" layoutY="29.0" mnemonicParsing="false" onAction="#backToWelcome" prefHeight="30.0" prefWidth="127.0" text="Back">
         <font>
            <Font name="System Italic" size="14.0" />
         </font></Button>
      <Label layoutX="53.0" layoutY="195.0" prefHeight="20.0" prefWidth="133.0" text=" Command Line">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>

      <TextField fx:id="d_CommandLine" layoutX="173.0" layoutY="192.0" prefHeight="26.0" prefWidth="426.0" promptText="Please Enter Command for Gameplay and Startup Phase" />
      <Button fx:id="d_FireCommand" layoutX="248.0" layoutY="233.0" mnemonicParsing="false" onAction="#getData" prefHeight="26.0" prefWidth="146.0" text="Execute Command">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></Button>
      <TextArea layoutX="110.0" layoutY="300.0" prefHeight="112.0" prefWidth="440.0" />
     
      <TextArea fx:id="d_showPlayPhase" layoutX="110.0" layoutY="300.0" prefHeight="135.0" prefWidth="440.0" />

   </children>
</AnchorPane>
