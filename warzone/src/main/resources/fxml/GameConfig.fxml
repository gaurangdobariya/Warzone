            <Font name="System Italic" size="14.0" />
         </font></Button>
      <Button fx:id="Back" layoutX="451.0" layoutY="29.0" mnemonicParsing="false" onAction="#backToWelcome" prefHeight="30.0" prefWidth="127.0" text="Back">
         <font>
            <Font name="System Italic" size="14.0" />
         </font></Button>
      <Label layoutY="195.0" prefHeight="20.0" prefWidth="133.0" text=" Command Line">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="d_CommandLine" layoutX="110.0" layoutY="192.0" prefHeight="26.0" prefWidth="440.0" promptText="Please Enter Command for Gameplay and Startup Phase" />
      <Button fx:id="FireCommand" layoutX="240.0" layoutY="234.0" mnemonicParsing="false" onAction="#getData" prefHeight="26.0" prefWidth="146.0" text="Execute Command" />
      <TextArea layoutX="110.0" layoutY="300.0" prefHeight="112.0" prefWidth="440.0" />
   </children>
</AnchorPane>
