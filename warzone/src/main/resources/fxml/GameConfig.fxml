<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="560.0" prefWidth="602.0" style="-fx-background-color: #CDCD14;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soen6441.warzone.controller.GameConfigController">
   <children>
      <Button fx:id="StartGame" layoutX="24.0" layoutY="29.0" mnemonicParsing="false" onAction="#toStartGame" prefHeight="30.0" prefWidth="127.0" text="Start Game">
