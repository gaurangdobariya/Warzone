<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="617.0" prefWidth="793.0" style="-fx-background-color: #CDCD14;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soen6441.warzone.controller.GameEngine">
   <children>

      <Button fx:id="d_BtnExit" layoutX="333.0" layoutY="23.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="30.0" prefWidth="128.0" text="Exit Game">
         <font>
            <Font name="System Italic" size="14.0" />
         </font></Button>
      <Label fx:id="d_playerTurn" layoutX="46.0" layoutY="160.0" prefHeight="20.0" prefWidth="174.0" text="                Command Line">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="d_CommandLine" alignment="CENTER" layoutX="222.0" layoutY="157.0" prefHeight="26.0" prefWidth="497.0" promptText="Enter the Command" />
      <Button fx:id="d_FireCommand" layoutX="432.0" layoutY="199.0" mnemonicParsing="false" onAction="#getData" prefHeight="30.0" prefWidth="162.0" text="Fire Command">
         <font>
            <Font size="14.0" />
         </font></Button>
      <TextArea fx:id="d_FireCommandList" layoutX="221.0" layoutY="257.0" prefHeight="308.0" prefWidth="498.0" wrapText="true" />
   </children>
</AnchorPane>
